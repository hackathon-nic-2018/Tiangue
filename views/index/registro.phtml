                        <input class="form-control" id="exampleInputLastName2" type="text" aria-describedby="nameHelp" placeholder="Segundo Apellido">
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="form-row">
                    <div class="col-md-3">
                        <label for="exampleInputName">Cedula</label>
                        <input class="form-control" id="exampleInputCedula" type="text" aria-describedby="nameHelp" placeholder="Cedula de Identidad">
                    </div>
                    <div class="col-md-3">
                        <label for="exampleInputLastName">Telefono</label>
                        <input class="form-control" id="exampleInputTelefono" type="text" aria-describedby="nameHelp" placeholder="Telefono">

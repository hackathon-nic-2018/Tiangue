                            <div class="col-md-6">
                                <label for="exampleInputPassword1">Contrase&ntildea</label>
                                <input class="form-control" id="exampleInputPassword1" type="password" placeholder="Contraseña">
                            </div>
                            <div class="col-md-6">
                                <label for="exampleConfirmPassword">Confirmar Contrase&ntildea</label>
                                <input class="form-control" id="exampleConfirmPassword" type="password" placeholder="Confirmar Contraseña">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-row">
                    <div class="col-md-3">
                        <label for="exampleInputName">Nombre de Usuario</label>
                        <input class="form-control" id="exampleInputusername" type="text" aria-describedby="nameHelp" placeholder="Usuario">
                    </div>
                    <div class="col-md-3">
                        <label for="exampleInputLastName">Clave</label>
                        <input class="form-control" id="exampleInputClave" type="text" aria-describedby="nameHelp" placeholder="Clave">
                    </div>

                    <div class="form-row">
                        <div class="col-md-3">
                            <label for="exampleInputEmail1">Correo Electronico</label>
                            <input class="form-control" id="exampleInputEmail1" type="email" aria-describedby="emailHelp" placeholder="Correo Electronico">
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="col-md-3">
                            <label for="exampleInputEmail1">Imagen Banner</label>
                            <input class="" id="exampleInputBanner" type="file" aria-describedby="emailHelp" placeholder="Correo Electronico">
                        </div>
                    </div>

                </div>

            </div>



            <div class="form-row">
                <div class="form-group">
                    <div class="col-md-4 col-lg-offset-4">
                        <br>
                        <button type="button" class="btn btn-primary btn-block" >Registrar</button>
                        <br>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>